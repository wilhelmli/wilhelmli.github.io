" value=\"0.5\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row\">\n            <div class=\"col-12 col-lg-6\" mbr-if=\"cardsAmount > 1\">\n                <div class=\"card-wrapper\">\n                    <div class=\"row\">\n                        <div class=\"col-12 col-md-7\">\n                            <div class=\"text-wrapper\">\n                                <h6 class=\"card-subtitle mbr-fonts-style mb-2\" mbr-theme-style=\"display-4\" mbr-if=\"showsubTitle\" data-app-selector=\".card-subtitle\"><b>\n                                    Lessons</b></h6>\n                                <h5 class=\"card-title mbr-fonts-style\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\" data-app-selector=\".card-title\"><a href=\"https://www.notion.so/wilhelmli/Wilhelm-s-Office-175613570c704f8f93a0d19bba784c3d\" class=\"text-black\" target=\"_blank\"><b>Wilhelm's Office</b></a></h5>\n                                <p class=\"mbr-text mbr-fonts-style mb-5\" mbr-theme-style=\"display-4\" mbr-if=\"showText\" data-app-selector=\".mbr-text, .mbr-section-btn\"><a href=\"https://www.notion.so/wilhelmli/Wilhelm-s-Office-175613570c704f8f93a0d19bba784c3d\" class=\"text-black\" target=\"_blank\">Visit Wilhelm's Office website to find out about Topics, Lesson plans, Group projects...</a></p>\n                                <div mbr-buttons mbr-theme-style=\"display-4\" class=\"mbr-section-btn\" mbr-if=\"showButtons\" data-toolbar=\"-mbrBtnMove\"><a href=\"https://www.notion.so/wilhelmli/Wilhelm-s-Office-175613570c704f8f93a0d19bba784c3d\" class=\"btn btn-primary\" data-app-placeholder=\"Type Text\" target=\"_blank\">⮕ Enter</a></div>\n                            </div>\n                   